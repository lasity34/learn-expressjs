<div class="row">
  <h1>Actions</h1>

  <table>
    <thead>
      <tr>
        <td>Type</td>
        <td>Price</td>
        <td>When</td>
      </tr>
    </thead>
    <tbody>
      {{#each actions}}
      <tr>
          <td>{{type}}</td>
          <td>R{{cost}}</td>
          <td>{{timestamp}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

